<h1>User index</h1>

	<p>Positive user</p>
		
	<% @users_pos.each do |user_pos| %>
		<li>
			<%= user_pos.name %>:
			<%= link_to "detail", "/users/#{user_pos.id}" %>
		</li>
	<% end %>

	<p>Negative user</p>
	<% @users_neg.each do |user_neg| %>
		<li>
			<%= user_neg.name %>:
			<%= link_to "detail", "/users/#{user_neg.id}" %>
		</li>
	<% end %>